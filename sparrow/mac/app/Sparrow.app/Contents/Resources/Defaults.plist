	<true/>
	<key>ShowFullSubject</key>
	<true/>
	<key>SoundOnNewMessage</key>
	<true/>
	<key>WebContinuousSpellCheckingEnabled</key>
	<true/>
	<key>UnifiedInboxEnabled</key>
	<true/>
	<key>HideInboxSelectionBar</key>
	<true/>
	<key>SidebarEnabled</key>
	<true/>
	<key>WindowAnimationDisabled</key>
	<true/>
	<key>AdsDisabled</key>
	<true/>
	<key>CloudUploadEnabled</key>
	<true/>
	<key>CloudUploadSoundEnabled</key>
	<true/>
	<key>RemoteImagesEnabled</key>
	<true/>
	<key>CFNetworkSocketEnabled</key>
	<false/>
	<key>AutomaticFallbackFetchAllContents</key>
	<true/>
</dict>
</plist>
