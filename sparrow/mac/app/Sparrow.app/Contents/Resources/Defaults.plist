<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>DebugMenuEnabled</key>
	<false/>
	<key>GrowlEnabled</key>
	<true/>
	<key>LEPLogEnabledFilenames</key>
	<dict/>
	<key>MMLogOutputFilenameKey-Disabled</key>
	<string>~/Library/Logs/Sparrow/Sparrow-%date%.log</string>
